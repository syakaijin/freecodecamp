function unite(arr1, arr2, arr3) {
  
  for(i=0; i<arr2.length; i++) {
    if(arr1.indexOf(arr2[i]) === -1) {
      arr1.push(arr2[i]);
    }
  }
  
  for(i=0; i<arr3.length; i++) {
    if(arr1.indexOf(arr3[i]) === -1) {
      arr1.push(arr3[i]);
    }
  }
  
  return arr1;
}

unite([1, 3, 2], [1, [1,5]], [2, [4]]);
